{
  "include": [
    "types/**/*",
    ".react-router/types/**/*",
    "app/**/*",
    "app/**/.server/**/*",
    "app/**/.client/**/*",
    "server/**/*"
  ],
  "compilerOptions": {
    "composite": true,
    "strict": true,
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "types": [
      "vite/client"
    ],
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "jsx": "react-jsx",
    "baseUrl": ".",
    "rootDirs": [
      ".",
      "./.react-router/types"
    ],
    "paths": {
      "~/*": [
        "./app/*"
      ]
    },
    "esModuleInterop": true,
    "resolveJsonModule": true
  }
}